{
  "name": "klass-subsets-client",
  "version": "1.0.8",
  "description": "A webclient application to communicate with Klass subsets service REST API in order to create and update classification subsets of codes.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/statisticsnorway/klass-subsets-client.git"
  },
  "keywords": [
    "klass",
    "subsets",
    "client",
    "react"
  ],
  "author": "SSB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/statisticsnorway/klass-subsets-client/issues"
  },
  "dependencies": {
    "@statisticsnorway/ssb-component-library": "^2.0.92",
    "dompurify": "^3.0.3",
    "feather-icons": "^4.29.1",
    "i18next": "^22.5.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^5.3.4",
    "swr": "^2.1.5"
  },
  "resolutions": {
    "autoprefixer": "10.4.5"
  },
  "scripts": {
    "prestart": "react-env --dest ./public",
    "start": "react-scripts start",
    "build": "react-scripts build && react-env --dest ./build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage --ci --testResultsProcessor=jest-junit-reporter",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@beam-australia/react-env": "^3.1.1",
    "jest-junit-reporter": "^1.1.0",
    "postcss": "^8.4.31",
    "react-scripts": "^5.0.1",
    "sass": "^1.70.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/**/index.js",
      "!src/images/**",
      "!src/internationalization/**",
      "!src/__tests__/test-data/**",
      "!src/components/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 4,
        "functions": 4,
        "lines": 4,
        "statements": 4
      }
    },
    "coverageReporters": [
      "cobertura",
      "lcov",
      "text"
    ]
  }
}
